{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "semi": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "max-len": ["error", { "code": 110 }],
    "no-shadow": "off"
  },
  "globals": {
    "fetch": false,
    "document": true
  }
}
